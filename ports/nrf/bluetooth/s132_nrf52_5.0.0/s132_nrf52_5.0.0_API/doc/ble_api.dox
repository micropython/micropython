 * APP=>SD [label = "sd_ble_gap_sec_info_reply(ediv, rand, LTK)"];
 * APP<<SD [label = "NRF_SUCCESS"];
 * SD:>CENTRAL [label = "LL Encryption Response", textcolor="#000080", linecolor="#000080"];
